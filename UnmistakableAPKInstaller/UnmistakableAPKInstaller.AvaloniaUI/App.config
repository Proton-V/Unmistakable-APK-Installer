<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="Aapt2DownloadLink" type="System.Configuration.AppSettingsSection" />
		<section name="AndroidPlatformToolsDownloadLink" type="System.Configuration.AppSettingsSection" />
	</configSections>

	<Aapt2DownloadLink>
		<add key="Windows"
			 value="https://dl.google.com/dl/android/maven2/com/android/tools/build/aapt2/3.2.0-alpha18-4804415/aapt2-3.2.0-alpha18-4804415-windows.jar"/>
		<add key="OSx"
			 value="https://dl.google.com/dl/android/maven2/com/android/tools/build/aapt2/3.2.0-alpha18-4804415/aapt2-3.2.0-alpha18-4804415-osx.jar"/>
		<add key="Linux"
