	</Aapt2DownloadLink>

	<AndroidPlatformToolsDownloadLink>
		<add key="Windows"
			 value="https://dl.google.com/android/repository/platform-tools_r33.0.3-windows.zip"/>
		<add key="OSx"
			 value="https://dl.google.com/android/repository/platform-tools_r33.0.3-darwin.zip"/>
		<add key="Linux"
			 value="https://dl.google.com/android/repository/platform-tools_r33.0.3-linux.zip"/>		
	</AndroidPlatformToolsDownloadLink>

	<appSettings>
