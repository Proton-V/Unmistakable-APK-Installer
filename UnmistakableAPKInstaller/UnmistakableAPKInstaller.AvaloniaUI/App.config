		<add key="DeviceLogDefaultFolderName"
			value="DeviceLog"/>
		<add key="DeviceLogFolderPath"
			value=""/>
		
		<add key="GoogleDriveApiKey"
			value="AIzaSyCmHbWyI3mhk4cV4LR1XUm4GWLiBqdjIoI"/>

		<add key="DiskCacheFileName"
			value="DiskCache.txt"/>
		<add key="LogFileName"
			value="UnmistakableAPKInstallerLog.log"/>
		<add key="AutoDelPrevApp"
			value="True"/>
	</appSettings>
</configuration>